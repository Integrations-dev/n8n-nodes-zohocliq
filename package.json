{
    "name": "n8n-nodes-zohocliq",
    "version": "1.0.1",
    "description": "This is an n8n community node. It lets you use Zoho Cliq in your n8n workflows",
    "license": "MIT",
    "homepage": "",
    "keywords": [
        "n8n-community-node-package"
    ],
    "author": {
        "name": "Integrations",
        "email": "integrations@cliqgeeks.com"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Integrations-dev/n8n-nodes-zohocliq.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": false,
        "credentials": [
            "dist/credentials/ZohoCliqOAuth2Api.credentials.js"
        ],
        "nodes": [
            "dist/nodes/ZohoCliq/ZohoCliq.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
